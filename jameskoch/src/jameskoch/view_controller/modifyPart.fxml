<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jameskoch.view_controller.ModifyPartController">
   <children>
      <AnchorPane prefHeight="500.0" prefWidth="500.0" />
      <AnchorPane layoutX="10.0" layoutY="-2.0" prefHeight="500.0" prefWidth="500.0">
         <children>
            <Label layoutX="14.0" layoutY="14.0" text="Modify Part">
               <font>
                  <Font name="System Bold Italic" size="18.0" />
               </font>
            </Label>
            <RadioButton fx:id="InHouseRadioButton" layoutX="162.0" layoutY="19.0" mnemonicParsing="false" onAction="#ModifyPartsInHouseRadio" text="In-House">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </RadioButton>
            <RadioButton fx:id="OutSourcedRadioButton" layoutX="318.0" layoutY="19.0" mnemonicParsing="false" onAction="#ModifyPartsOutsourcedRadio" text="Oursourced">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </RadioButton>
            <Label layoutX="61.0" layoutY="64.0" text="ID" />
            <Label layoutX="61.0" layoutY="112.0" text="Name" />
            <Label layoutX="61.0" layoutY="159.0" text="Inv" />
            <Label layoutX="61.0" layoutY="202.0" text="Price/Cost" />
            <Label layoutX="281.0" layoutY="252.0" text="Max" />
            <Label fx:id="CoNameMachIdLabel" layoutX="61.0" layoutY="303.0" text="Company Name" />
            <TextField fx:id="IdTextField" layoutX="199.0" layoutY="60.0" promptText="ID" />
            <TextField fx:id="NameTextField" layoutX="199.0" layoutY="108.0" promptText="Name" />
            <TextField fx:id="InventoryTextField" layoutX="199.0" layoutY="155.0" promptText="Inventory" />
            <TextField fx:id="PriceTextField" layoutX="199.0" layoutY="198.0" promptText="Price/Cost" />
            <TextField fx:id="MaxTextField" layoutX="316.0" layoutY="248.0" prefHeight="25.0" prefWidth="65.0" promptText="Max Inv" />
            <TextField fx:id="CoNameMachIdTextField" layoutX="199.0" layoutY="299.0" />
            <Label layoutX="67.0" layoutY="252.0" text="Min" />
            <TextField fx:id="MinTextField" layoutX="203.0" layoutY="248.0" prefHeight="25.0" prefWidth="65.0" promptText="Min Inv" />
            <Button layoutX="292.0" layoutY="405.0" mnemonicParsing="false" onAction="#ModifyPartsSaveClicked" text="Save" />
            <Button layoutX="379.0" layoutY="405.0" mnemonicParsing="false" onAction="#ModifyPartsCancelClicked" text="Cancel" />
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </AnchorPane>
   </children>
</AnchorPane>
